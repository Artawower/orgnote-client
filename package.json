{
  "name": "orgnote-client",
  "version": "0.30.0",
  "description": "Org Roam implementation for web and mobile devices.",
  "productName": "orgnote",
  "author": "OrgNote <app.orgnote@gmail.com>",
  "type": "module",
  "private": false,
  "scripts": {
    "dev": "env-cmd -f ./.env quasar dev",
    "dev:remote": "env-cmd -f ./.env.prod quasar dev",
    "dev:android": " env-cmd -f ./.env.prod quasar dev -m capacitor -T android",
    "dev:ssr": "env-cmd -f ./.env.ssr quasar dev -m ssr",
    "dev:android:remote": "env-cmd -f ./.env.prod quasar dev -m capacitor -T android -- -d",
    "dev:ios": "quasar dev -m capacitor -T ios",
    "dev:ios:remote": "env-cmd -f ./.env.prod quasar dev -m capacitor -T ios -- --target=\"iPhone-15-Pro\"",
    "dev:ios-tablet:remote": "env-cmd -f ./.env.prod quasar dev -m capacitor -T ios -- --target=\"iPad-10th-generation\"",
    "dev:electron": "quasar dev -m electron",
    "dev:electron:remote": "env-cmd -f ./.env.prod quasar dev -m electron",
    "dev:ssr:build": "env-cmd -f ./.env.ssr node ./dist/ssr/index.js",
    "dev:pwa:remote": "npm run build:pwa && docker compose -f ./deploy/docker-compose.pwa.yaml up",
    "clean:android": "cd src-capacitor/android && ./gradlew clean && ./gradlew build --refresh-dependencies",
    "build": "quasar build",
    "build:ssr": "env-cmd -f ./.env.prod bun run build -m ssr",
    "build:electron": "env-cmd -f ./.env.prod bun run build -m electron",
    "build:all": "bun run build:pwa && bun run build:android:aab",
    "build:pwa": "env-cmd -f ./.env.prod quasar build -m pwa",
    "build:android": "env-cmd -f ./.env.prod quasar build -m capacitor -T android -- bundleRelease",
    "build:android:aab": "./deploy/sync-capaticor-version.sh && build:android",
    "build:android:aab:sign": "bun run build:android:aab && ./sign-aab.sh",
    "build:android:apk:sign": "bun run build:android:aab && ./sign-apk.sh",
    "build:ios": "env-cmd -f ./.env.prod quasar build -m capacitor -T ios",
    "report:duplicates": "jscpd ./src",
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "test": "vitest",
    "generate:icons": "icongenie generate --skip-trim -i images/modern-unicorn.png --png-color 9356E8 --splashscreen-color 9356E8 && icongenie generate --padding 70,70 -m capacitor -i images/modern-unicorn-round.png --png-color 9356E8 --splashscreen-color 9356E8",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@quasar/cli": "^2.5.0",
    "@quasar/extras": "1.16.17",
    "@vueuse/core": "12.8.2",
    "axios": "1.8.2",
    "dexie": "4.0.11",
    "fast-deep-equal": "3.1.3",
    "fuse.js": "7.1.0",
    "orgnote-api": "file:.yalc/orgnote-api",
    "pinia": "2.3.0",
    "pinia-plugin-persistedstate": "4.2.0",
    "quasar": "2.16.6",
    "register-service-worker": "^1.7.2",
    "rfdc": "1.4.1",
    "splitpanes": "3.1.8",
    "uuid": "11.1.0",
    "valibot": "1.0.0-rc.3",
    "vue": "3.5.13",
    "vue-highlight-words": "3.0.1",
    "vue-i18n": "10.0.5",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@capacitor/status-bar": "6.0.2",
    "@chromatic-com/storybook": "^3.2.5",
    "@electron/packager": "^18.3.6",
    "@eslint/js": "9.22.0",
    "@faker-js/faker": "9.6.0",
    "@hugotomazi/capacitor-navigation-bar": "4.0.1",
    "@intlify/unplugin-vue-i18n": "6.0.3",
    "@pinia/testing": "0.1.7",
    "@quasar/app-vite": "2.1.4",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-interactions": "^8.6.4",
    "@storybook/addon-onboarding": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@storybook/vue3": "^8.6.4",
    "@storybook/vue3-vite": "^8.6.4",
    "@types/bun": "1.2.4",
    "@types/node": "22.13.10",
    "@types/splitpanes": "2.2.6",
    "@types/uuid": "10.0.0",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.5.0",
    "@vue/test-utils": "2.4.6",
    "autoprefixer": "10.4.21",
    "electron": "33.2.1",
    "env-cmd": "10.1.0",
    "eslint": "9.22.0",
    "eslint-plugin-storybook": "^0.11.4",
    "eslint-plugin-vue": "9.32.0",
    "fake-indexeddb": "6.0.0",
    "globals": "15.14.0",
    "happy-dom": "16.5.2",
    "husky": "9.1.7",
    "jscpd": "4.0.5",
    "linkedom": "0.18.9",
    "prettier": "3.5.3",
    "storybook": "^8.6.4",
    "typescript": "~5.8.2",
    "vite": "6.2.1",
    "vite-plugin-checker": "0.9.0",
    "vitest": "2.1.8",
    "vue-tsc": "2.2.8",
    "workbox-build": "^7.3.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "jscpd": {
    "threshold": 1.65,
    "reporters": [
      "html",
      "console"
    ],
    "format": [
      "typescript"
    ],
    "ignore": [
      "**/__snapshots__/**",
      "**/.git/**",
      "**/*.spec.ts",
      "**/node_modules/**",
      "**/dist/**",
      "**/generated/**"
    ],
    "minLines": 3,
    "minTokens": 50,
    "ignorePattern": [
      "import.*from\\s*'.*'",
      "export.*from\\s*'.*'"
    ],
    "output": "./dist/duplication-report/",
    "absolute": true,
    "blame": true,
    "gitignore": true
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
