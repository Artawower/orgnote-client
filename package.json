{
  "name": "orgnote-client",
  "version": "0.0.1",
  "description": "Org Roam implementation for web and mobile devices.",
  "productName": "orgnote",
  "author": "Art <Artawower@mail.ru>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "report:duplicates": "jscpd ./src",
    "postinstall": "quasar prepare",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "1.2.1",
    "vue-i18n": "9.2.2",
    "pinia": "2.0.11",
    "@quasar/extras": "1.16.4",
    "quasar": "2.16.0",
    "vue": "3.4.18",
    "vue-router": "4.0.12"
  },
  "devDependencies": {
    "@eslint/js": "9.14.0",
    "@intlify/unplugin-vue-i18n": "2.0.0",
    "@quasar/app-vite": "2.0.0",
    "@types/node": "20.5.9",
    "@vue/eslint-config-prettier": "10.1.0",
    "@vue/eslint-config-typescript": "14.1.3",
    "autoprefixer": "10.4.2",
    "eslint": "9.14.0",
    "eslint-plugin-vue": "9.30.0",
    "globals": "15.12.0",
    "husky": "9.1.7",
    "jscpd": "4.0.5",
    "prettier": "3.3.3",
    "typescript": "~5.5.3",
    "vite-plugin-checker": "0.8.0",
    "vue-tsc": "2.0.29"
  },
  "jscpd": {
    "threshold": 1.65,
    "reporters": ["html", "console"],
    "format": ["typescript"],
    "ignore": [
      "**/__snapshots__/**",
      "**/.git/**",
      "**/*.spec.ts",
      "**/node_modules/**",
      "**/dist/**",
      "**/generated/**"
    ],
    "minLines": 3,
    "minTokens": 50,
    "ignorePattern": ["import.*from\\s*'.*'", "export.*from\\s*'.*'"],
    "output": "./dist/duplication-report/",
    "absolute": true,
    "blame": true,
    "gitignore": true
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
