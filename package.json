{
  "name": "second-brain-client",
  "version": "0.1.0",
  "description": "Second brain client for eponymous service",
  "productName": "Second brain",
  "author": "artawower <artawower@protonmail.com>",
  "private": true,
  "scripts": {
    "generate:css-doc": "node collect-css-variables.js",
    "serve:ssr": "node dist/ssr/index.js",
    "serve:android": "quasar dev -m cordova -T android",
    "serve:ios": "quasar dev -m cordova -T ios",
    "dev": "quasar dev",
    "build": "quasar build",
    "build:ssr": "yarn build -m ssr",
    "build:all": "yarn build:pwa && yarn build:ssr && yarn build:android",
    "build:pwa": "quasar build -m pwa",
    "build:android": "quasar build -m android",
    "build:ios": "quasar build -m ios",
    "codegen:api": "openapi-generator-cli generate",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .prettierignore",
    "test": "echo \"No test specified\" && exit 0",
    "prepare": "husky install"
  },
  "dependencies": {
    "@editorjs/editorjs": "2.26.5",
    "@editorjs/header": "2.7.0",
    "@editorjs/inline-code": "1.4.0",
    "@editorjs/nested-list": "1.3.0",
    "@editorjs/underline": "1.1.0",
    "@highlightjs/vue-plugin": "2.1.0",
    "@hsorby/vue3-katex": "0.6.0-rc.3",
    "@quasar/extras": "^1.0.0",
    "@sotaproject/strikethrough": "1.0.1",
    "axios": "0.27.2",
    "editorjs-undo": "2.0.21",
    "force-graph": "1.42.13",
    "highlight.js": "11.6.0",
    "katex": "0.15.3",
    "openapi-client-axios": "7.1.3",
    "org-mode-ast": "0.2.9",
    "pinia": "2.0.17",
    "pinia-plugin-persistedstate": "1.6.3",
    "postcss": "8.4.14",
    "prosemirror-schema-basic": "1.2.1",
    "prosemirror-state": "1.4.2",
    "prosemirror-view": "1.30.2",
    "quasar": "^2.7.2",
    "quill": "1.3.7",
    "rollup": "2.77.2",
    "slate": "0.93.0",
    "slate-vue": "0.2.0",
    "tinykeys": "1.4.0",
    "uuid": "9.0.0",
    "vue": "^3.0.0",
    "vue-i18n": "9.2.0-beta.35",
    "vue-router": "^4.0.0",
    "vue3-easy-data-table": "1.4.2"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "5.0.1",
    "@openapitools/openapi-generator-cli": "2.7.0",
    "@quasar/app-vite": "1.0.5",
    "@quasar/vite-plugin": "1.1.1",
    "@types/editorjs__header": "2.6.0",
    "@types/node": "18.6.2",
    "@types/quill": "2.0.10",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "autoprefixer": "10.4.7",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-vue": "9.3.0",
    "husky": "8.0.3",
    "openapi-client-axios-typegen": "7.2.0",
    "prettier": "2.7.1",
    "sass": "1.32.12",
    "swagger-axios-codegen": "0.15.11",
    "typescript": "4.7.4",
    "vue/compiler-sfc": "npm:@vue/compiler-sfc@^3",
    "workbox-build": "6.5.4",
    "workbox-cacheable-response": "6.5.4",
    "workbox-core": "6.5.4",
    "workbox-expiration": "6.5.4",
    "workbox-precaching": "6.5.4",
    "workbox-routing": "6.5.4",
    "workbox-strategies": "6.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
