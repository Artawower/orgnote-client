{
  "name": "orgnote-client",
  "version": "0.8.29",
  "description": "Org Note client for eponymous service",
  "productName": "Org Note",
  "author": "artawower <artawower@protonmail.com>",
  "private": true,
  "scripts": {
    "codegen:css-doc": "node collect-css-variables.js",
    "serve:ssr": "node dist/ssr/index.js",
    "dev:android": "quasar dev -m cordova -T android",
    "dev:android:remote": "export API_URL=https://org-note.com/api && export AUTH_URL=https://org-note.com && quasar dev -m cordova -T android -- -d",
    "dev:ios": "quasar dev -m cordova -T ios",
    "dev:ios:remote": "export API_URL=https://org-note.com/api && export AUTH_URL=https://org-note.com && quasar dev -m cordova -T ios",
    "dev:electron": "quasar dev -m electron",
    "dev": "quasar dev",
    "dev:remote": "export API_URL=https://org-note.com/api && AUTH_URL=https://org-note.com && quasar dev",
    "build": "quasar build",
    "build:ssr": "bun run build -m ssr",
    "build:electron": "bun run build -m electron",
    "build:all": "bun run build:pwa && bun run build:ssr && bun run build:android",
    "build:pwa": "quasar build -m pwa",
    "build:android": "quasar build -m android",
    "build:ios": "quasar build -m ios",
    "codegen:api": "openapi-generator-cli generate",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .prettierignore",
    "test": "bun test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@editorjs/header": "2.7.0",
    "@editorjs/inline-code": "1.4.0",
    "@editorjs/nested-list": "1.3.0",
    "@editorjs/underline": "1.1.0",
    "@highlightjs/vue-plugin": "2.1.0",
    "@hsorby/vue3-katex": "0.6.0-rc.3",
    "@lezer/javascript": "1.4.7",
    "@lezer/python": "1.1.8",
    "@quasar/extras": "^1.0.0",
    "@sotaproject/strikethrough": "1.0.1",
    "@uiw/codemirror-extensions-langs": "4.21.18",
    "axios": "1.5.0",
    "codemirror": "6.0.1",
    "dexie": "4.0.1-alpha.25",
    "editorjs-undo": "2.0.21",
    "force-graph": "1.42.13",
    "highlight.js": "11.6.0",
    "katex": "0.15.3",
    "openapi-client-axios": "7.1.3",
    "org-mode-ast": "0.8.21",
    "pinia": "2.0.17",
    "pinia-plugin-persistedstate": "1.6.3",
    "postcss": "8.4.14",
    "prosemirror-schema-basic": "1.2.1",
    "prosemirror-state": "1.4.2",
    "prosemirror-view": "1.30.2",
    "quasar": "^2.7.2",
    "rollup": "2.77.2",
    "slate": "0.93.0",
    "slate-vue": "0.2.0",
    "tinykeys": "1.4.0",
    "uuid": "9.0.0",
    "vue": "^3.3.4",
    "vue-i18n": "9.4.1",
    "vue-router": "^4.0.0",
    "vue3-easy-data-table": "1.5.47"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "5.0.1",
    "@openapitools/openapi-generator-cli": "2.7.0",
    "@quasar/app-vite": "1.6.2",
    "@quasar/vite-plugin": "1.1.1",
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@types/editorjs__header": "2.6.0",
    "@types/jest": "29.5.5",
    "@types/node": "18.6.2",
    "@types/uuid": "9.0.4",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "autoprefixer": "10.4.7",
    "electron": "^26.2.4",
    "electron-packager": "^17.1.1",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-vue": "9.3.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "openapi-client-axios-typegen": "7.2.0",
    "prettier": "2.7.1",
    "sass": "1.32.12",
    "swagger-axios-codegen": "0.15.11",
    "typescript": "4.7.4",
    "workbox-build": "6.5.4",
    "workbox-cacheable-response": "6.5.4",
    "workbox-core": "6.5.4",
    "workbox-expiration": "6.5.4",
    "workbox-precaching": "6.5.4",
    "workbox-routing": "6.5.4",
    "workbox-strategies": "6.5.4"
  },
  "engines": {
    "bun": ">= 1.0.2"
  }
}
