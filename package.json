{
  "name": "orgnote-client",
  "version": "0.15.3",
  "description": "OrgNote app. OrgNote is a note-taking app for organizing your thoughts and ideas. It is based on the emacs org-roam package and org-mode format.",
  "productName": "OrgNote",
  "author": "OrgNote <app.orgnote@gmail.com>",
  "private": true,
  "scripts": {
    "serve:ssr": "node dist/ssr/index.js",
    "dev:android": "quasar dev -m cordova -T android",
    "dev:android:remote": "env-cmd -f ./.env.prod quasar dev -m cordova -T android -- -d",
    "dev:ios": "quasar dev -m cordova -T ios",
    "dev:ios:remote": "env-cmd -f ./.env.prod quasar dev -m cordova -T ios -- --target=\"iPhone-15-Pro\"",
    "dev:electron": "quasar dev -m electron",
    "dev:electron:remote": "env-cmd -f ./.env.prod quasar dev -m electron",
    "dev": "env-cmd -f ./.env quasar dev",
    "dev:remote": "env-cmd -f ./.env.prod quasar dev",
    "build": "quasar build",
    "build:ssr": "./.env.prod bun run build -m ssr",
    "build:electron": "env-cmd -f ./.env.prod bun run build -m electron",
    "build:all": "bun run build:pwa && bun run build:android",
    "build:pwa": "env-cmd -f ./.env.prod quasar build -m pwa",
    "sign:android": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore org-note-release.keystore -signedjar org-note-release-signed.aab ./dist/cordova/android/bundle/release/app-release.aab sb",
    "build:android:sign": "bun run build:android && bun run sign:android",
    "build:android": "env-cmd -f ./.env.prod quasar build -m android",
    "build:ios": "env-cmd -f ./.env.prod quasar build -m ios",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .prettierignore",
    "test": "bun test",
    "prepare": "husky install",
    "report:duplicates": "jscpd ./src"
  },
  "dependencies": {
    "@electron/remote": "2.1.2",
    "@es-git/fetch-mixin": "0.10.0",
    "@es-git/idb-repo": "0.10.0",
    "@es-git/load-as-mixin": "0.10.0",
    "@es-git/memory-repo": "0.10.0",
    "@es-git/object-mixin": "0.10.0",
    "@es-git/walkers-mixin": "0.10.0",
    "@highlightjs/vue-plugin": "2.1.0",
    "@hsorby/vue3-katex": "0.6.0-rc.7",
    "@lezer/common": "1.1.0",
    "@lezer/javascript": "1.4.7",
    "@lezer/python": "1.1.8",
    "@quasar/extras": "^1.16.9",
    "@sotaproject/strikethrough": "1.0.1",
    "@uiw/codemirror-extensions-langs": "4.21.18",
    "axios": "1.6.7",
    "codemirror": "6.0.1",
    "dexie": "4.0.1-alpha.27",
    "editorjs-undo": "2.0.26",
    "force-graph": "1.43.4",
    "highlight.js": "11.9.0",
    "katex": "0.16.9",
    "openapi-client-axios": "7.5.4",
    "org-mode-ast": "0.11.2",
    "orgnote-api": "0.2.0",
    "pinia": "2.1.7",
    "pinia-plugin-persistedstate": "3.2.1",
    "postcss": "8.4.33",
    "quasar": "^2.14.3",
    "rollup": "4.9.6",
    "slate": "0.101.5",
    "slate-vue": "0.2.0",
    "tinykeys": "2.1.0",
    "uuid": "9.0.1",
    "vue": "^3.4.15",
    "vue-i18n": "9.9.0",
    "vue-router": "^4.2.5",
    "vue3-easy-data-table": "1.5.12",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "6.0.3",
    "@quasar/app-vite": "1.7.3",
    "@quasar/vite-plugin": "1.6.0",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/editorjs__header": "2.6.3",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.10",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "autoprefixer": "10.4.17",
    "commander": "11.1.0",
    "electron": "28.2.0",
    "electron-builder": "24.9.1",
    "electron-packager": "17.1.2",
    "env-cmd": "10.1.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-vue": "9.20.1",
    "husky": "9.0.7",
    "jest": "29.7.0",
    "jscpd": "3.5.10",
    "openapi-client-axios-typegen": "7.6.0",
    "prettier": "3.2.4",
    "sass": "1.70.0",
    "swagger-axios-codegen": "0.16.8",
    "typescript": "5.3.3",
    "vite-plugin-checker": "0.6.2",
    "vue-tsc": "1.8.27",
    "workbox-build": "7.0.0",
    "workbox-cacheable-response": "7.0.0",
    "workbox-core": "7.0.0",
    "workbox-expiration": "7.0.0",
    "workbox-precaching": "7.0.0",
    "workbox-routing": "7.0.0",
    "workbox-strategies": "7.0.0"
  },
  "engines": {
    "bun": ">= 1.0.2"
  },
  "jscpd": {
    "threshold": 1.5,
    "reporters": ["html", "console"],
    "format": ["typescript"],
    "ignore": [
      "**/__snapshots__/**",
      "**/.git/**",
      "**/*.spec.ts",
      "**/node_modules/**",
      "**/dist/**",
      "**/generated/**"
    ],
    "minLines": 3,
    "minTokens": 50,
    "ignorePattern": ["import.*from\\s*'.*'", "export.*from\\s*'.*'"],
    "output": "./dist/duplication-report/",
    "absolute": true,
    "blame": true,
    "gitignore": true
  }
}
